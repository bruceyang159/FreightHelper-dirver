    <string-array name="StoresApprove">
        <item>待审核</item>
        <item>已审核</item>
    </string-array>

    <string-array name="Date">
        <item>今天</item>
        <item>昨天</item>
        <item>近一周</item>
        <item>三个月</item>
    </string-array>

</resources>